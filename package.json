{
  "name": "bachelor-denfese-server",
  "version": "0.2.0",
  "description": "YunBackup Server with Koa2",
  "main": "index.js",
  "private": true,
  "scripts": {
    "start": "node index.js",
    "dev": "./node_modules/.bin/nodemon index.js",
    "test": "NODE_ENV=test ./node_modules/.bin/mocha --compilers js:babel-register --require babel-polyfill",
    "lint": "eslint src/**/*.js",
    "docs": "./node_modules/.bin/apidoc -i src/ -o docs",
    "mongo": "mongod --dbpath ./dbdata",
    "clean:log": "rm -rf npm-debug.log**",
    "clean:file": "rm -rf upload_files/**"
  },
  "author": "Warrior! <wubocong.xyz@gmail.com>",
  "apidoc": {
    "title": "bachelor-denfese-server",
    "url": "localhost:5000"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "dependencies": {
    "apidoc": "^0.16.1",
    "async-busboy": "^0.3.3",
    "babel-core": "^6.18.2",
    "babel-polyfill": "^6.16.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-stage-0": "^6.16.0",
    "bcrypt": "^0.8.7",
    "glob": "^7.1.1",
    "jsonwebtoken": "^7.1.9",
    "koa": "^2.0.0-alpha.7",
    "koa-better-body": "^3.0.2",
    "koa-convert": "^1.2.0",
    "koa-cors": "0.0.16",
    "koa-generic-session": "^1.11.3",
    "koa-logger": "^2.0.0",
    "koa-mount": "^1.3.0",
    "koa-passport": "^2.0.1",
    "koa-router": "^7.0.1",
    "koa-static": "^2.0.0",
    "mongoose": "^4.6.6",
    "passport-local": "1.0.0"
  },
  "devDependencies": {
    "babel-eslint": "^7.1.0",
    "babel-register": "^6.18.0",
    "chai": "^3.5.0",
    "cross-env": "^3.1.3",
    "eslint": "^3.9.1",
    "eslint-config-standard": "^6.2.1",
    "eslint-plugin-promise": "^3.3.1",
    "eslint-plugin-standard": "^2.0.1",
    "mocha": "^3.1.2",
    "nodemon": "^1.11.0",
    "supertest": "^2.0.1"
  }
}
